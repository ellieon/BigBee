{
  "name": "big-dick-bee",
  "version": "1.0.0",
  "description": "Everybody knows it's big dick bee!",
  "author": "Ellie Harrison",
  "dependencies": {
    "@types/node": "^14.14.6",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.2",
    "db-migrate": "^0.11.11",
    "db-migrate-pg": "^1.2.2",
    "discord-oauth2": "^2.5.0",
    "discord.js": "^12.4.1",
    "express": "^4.17.1",
    "express-nunjucks": "^2.2.5",
    "jsonwebtoken": "^8.5.1",
    "nunjucks": "^3.2.2",
    "pg": "^8.4.2",
    "request": "^2.88.2",
    "spotify-web-api-node": "^5.0.0",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5",
    "winston": "^3.3.3"
  },
  "scripts": {
    "start": "db-migrate up && node -r tsconfig-paths/register server.js --trace-warnings",
    "build": "tsc",
    "build_start": "npm run build && npm run start",
    "clean": "tsc --build --clean",
    "postinstall": "npm run build",
    "start-dev": "db-migrate up && ts-node -r tsconfig-paths/register server.ts",
    "lint": "tsc --noEmit && tslint --project tsconfig.json",
    "test": "cross-env NODE_ENV=test ts-mocha --recursive -r tsconfig-paths/register -p tsconfig.json src/**/*.spec.ts --exit"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.0.3",
    "chai": "^4.2.0",
    "husky": "^4.3.0",
    "mocha": "^8.2.1",
    "mock-discordjs": "^0.2.0",
    "sinon": "^9.2.1",
    "sinon-chai": "^3.5.0",
    "supertest": "^6.0.1",
    "ts-mocha": "^8.0.0",
    "ts-mock-imports": "^1.3.0",
    "ts-node-dev": "^1.0.0",
    "tslint": "^6.1.2",
    "tslint-config-standard": "^9.0.0",
    "tslint-microsoft-contrib": "^6.2.0"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "engineStrict": true,
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    }
  }
}
